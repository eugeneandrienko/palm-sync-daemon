PROJECT_NAME           = @PACKAGE_NAME@
PROJECT_NUMBER         = @PACKAGE_VERSION@
PROJECT_BRIEF          = Simple synchronization between Palm device and OrgMode files
INPUT                  = @top_srcdir@/include @top_srcdir@/src
RECURSIVE              = yes
EXCLUDE_PATTERNS       = */tests/*
EXCLUDE_PATTERNS      += */orgmode/parser/parser.*
EXCLUDE_PATTERNS      += */orgmode/parser/scanner.*
OPTIMIZE_OUTPUT_FOR_C  = yes
INLINE_SIMPLE_STRUCTS  = yes
TYPEDEF_HIDES_STRUCT   = yes
SHOW_INCLUDE_FILES     = no
BRIEF_MEMBER_DESC      = yes
SHOW_USED_FILES        = no
ENABLE_PREPROCESSING   = yes
PREDEFINED             = DOXYGEN_SHOULD_SKIP_THIS
JAVADOC_AUTOBRIEF      = yes
WARN_AS_ERROR          = yes
DISABLE_INDEX          = yes
GENERATE_TREEVIEW      = yes
FULL_SIDEBAR           = no
FULL_PATH_NAMES        = no
SORT_MEMBER_DOCS       = no
GENERATE_HTML          = yes
GENERATE_TREEVIEW      = yes
GENERATE_LATEX         = no
GENERATE_RTF           = no
GENERATE_XML           = no
GENERATE_MAN           = no
GENERATE_DOCBOOK       = no
